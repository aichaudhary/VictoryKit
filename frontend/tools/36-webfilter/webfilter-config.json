{
  "toolId": 36,
  "toolName": "WebFilter",
  "displayName": "WebFilter - Intelligent Web Content Filtering",
  "description": "Enterprise-grade web filtering and content control platform with AI-powered URL analysis, real-time threat detection, and comprehensive policy management",
  "version": "1.0.0",
  "category": "Security & Compliance",
  "subdomain": "webfilter.maula.ai",
  "theme": {
    "primaryColor": "#10b981",
    "secondaryColor": "#059669",
    "accentColor": "#34d399",
    "darkColor": "#047857",
    "lightColor": "#d1fae5"
  },
  "ports": {
    "frontend": 3036,
    "api": 4036,
    "aiAssistant": 6036,
    "mlEngine": 8036
  },
  "database": {
    "name": "webfilter_db",
    "collections": [
      "url_analyses",
      "filter_policies",
      "user_profiles",
      "access_logs",
      "alerts",
      "reports",
      "category_rules",
      "whitelist",
      "blacklist"
    ]
  },
  "aiAssistant": {
    "enabled": true,
    "wsEndpoint": "ws://localhost:6036",
    "model": "gemini-1.5-pro",
    "context": "WebFilter AI Assistant - Intelligent web content filtering and threat analysis"
  },
  "features": {
    "urlAnalysis": {
      "enabled": true,
      "realTimeScanning": true,
      "deepInspection": true,
      "contentClassification": true,
      "threatDetection": true,
      "reputationScoring": true
    },
    "contentFiltering": {
      "enabled": true,
      "categories": [
        "adult",
        "gambling",
        "malware",
        "phishing",
        "weapons",
        "drugs",
        "violence",
        "hate_speech",
        "social_media",
        "streaming",
        "gaming",
        "shopping",
        "news",
        "education",
        "business"
      ],
      "customCategories": true,
      "keywordFiltering": true,
      "imageAnalysis": true
    },
    "policyManagement": {
      "enabled": true,
      "userBasedPolicies": true,
      "groupPolicies": true,
      "timeBasedRules": true,
      "bandwidthControl": true,
      "quotaManagement": true,
      "bypassRules": true
    },
    "threatProtection": {
      "enabled": true,
      "malwareBlocking": true,
      "phishingDetection": true,
      "botnetProtection": true,
      "cryptominingDetection": true,
      "zeroDay Protection": true
    },
    "monitoring": {
      "enabled": true,
      "realTimeAlerts": true,
      "activityLogging": true,
      "userBehavior": true,
      "trafficAnalysis": true,
      "complianceReporting": true
    },
    "compliance": {
      "enabled": true,
      "standards": [
        "CIPA",
        "COPPA",
        "GDPR",
        "HIPAA",
        "PCI-DSS",
        "SOX"
      ],
      "auditLogging": true,
      "dataRetention": true,
      "privacyControls": true
    }
  },
  "integrations": {
    "threatIntelligence": [
      "google_safe_browsing",
      "virustotal",
      "abuseipdb",
      "phishtank",
      "urlscan_io",
      "cisco_talos",
      "alienvault_otx"
    ],
    "contentClassification": [
      "openai_moderation",
      "google_cloud_vision",
      "aws_rekognition"
    ],
    "authentication": [
      "active_directory",
      "ldap",
      "saml",
      "oauth2",
      "radius"
    ],
    "siem": [
      "splunk",
      "elk_stack",
      "azure_sentinel"
    ]
  },
  "aiFunctions": [
    {
      "name": "analyze_url_threat",
      "description": "Comprehensive URL threat analysis including malware, phishing, and reputation scoring",
      "parameters": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL to analyze for threats"
          },
          "deepScan": {
            "type": "boolean",
            "description": "Perform deep content inspection",
            "default": false
          },
          "checkReputation": {
            "type": "boolean",
            "description": "Check domain reputation across threat databases",
            "default": true
          }
        },
        "required": ["url"]
      }
    },
    {
      "name": "classify_content_category",
      "description": "Classify web content into appropriate categories for filtering decisions",
      "parameters": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL to classify"
          },
          "content": {
            "type": "string",
            "description": "Page content for analysis"
          },
          "includeImages": {
            "type": "boolean",
            "description": "Include image analysis in classification",
            "default": false
          }
        },
        "required": ["url"]
      }
    },
    {
      "name": "detect_bypass_attempt",
      "description": "Detect attempts to bypass web filtering using proxies, VPNs, or encoded URLs",
      "parameters": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL to check for bypass techniques"
          },
          "userAgent": {
            "type": "string",
            "description": "User agent string from request"
          },
          "headers": {
            "type": "object",
            "description": "HTTP headers for analysis"
          }
        },
        "required": ["url"]
      }
    },
    {
      "name": "analyze_user_behavior",
      "description": "Analyze user web browsing patterns for anomaly detection and risk assessment",
      "parameters": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "User identifier"
          },
          "timePeriod": {
            "type": "string",
            "enum": ["hour", "day", "week", "month"],
            "description": "Time period for analysis"
          },
          "includePatterns": {
            "type": "boolean",
            "description": "Include detailed pattern analysis",
            "default": true
          }
        },
        "required": ["userId", "timePeriod"]
      }
    },
    {
      "name": "generate_policy_recommendation",
      "description": "Generate intelligent policy recommendations based on usage patterns and threats",
      "parameters": {
        "type": "object",
        "properties": {
          "organizationType": {
            "type": "string",
            "enum": ["enterprise", "education", "government", "healthcare"],
            "description": "Organization type for tailored recommendations"
          },
          "currentThreats": {
            "type": "array",
            "description": "Current threat landscape data"
          },
          "complianceRequirements": {
            "type": "array",
            "description": "Required compliance standards"
          }
        },
        "required": ["organizationType"]
      }
    },
    {
      "name": "investigate_suspicious_activity",
      "description": "Deep investigation of suspicious web activity patterns across users and time",
      "parameters": {
        "type": "object",
        "properties": {
          "activityId": {
            "type": "string",
            "description": "Activity identifier to investigate"
          },
          "timeRange": {
            "type": "object",
            "properties": {
              "start": {
                "type": "string",
                "description": "Start timestamp"
              },
              "end": {
                "type": "string",
                "description": "End timestamp"
              }
            }
          },
          "correlateUsers": {
            "type": "boolean",
            "description": "Correlate activity across multiple users",
            "default": false
          }
        },
        "required": ["activityId"]
      }
    },
    {
      "name": "assess_content_risk",
      "description": "Assess risk level of web content for organizational security and compliance",
      "parameters": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL to assess"
          },
          "contentType": {
            "type": "string",
            "description": "MIME type of content"
          },
          "organizationProfile": {
            "type": "string",
            "enum": ["high_security", "standard", "permissive"],
            "description": "Organization's risk profile"
          }
        },
        "required": ["url"]
      }
    },
    {
      "name": "analyze_bandwidth_usage",
      "description": "Analyze bandwidth consumption patterns and identify optimization opportunities",
      "parameters": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "User or group identifier"
          },
          "timePeriod": {
            "type": "string",
            "enum": ["hour", "day", "week", "month"],
            "description": "Analysis time period"
          },
          "categorize": {
            "type": "boolean",
            "description": "Categorize bandwidth by content type",
            "default": true
          }
        },
        "required": ["timePeriod"]
      }
    },
    {
      "name": "detect_data_exfiltration",
      "description": "Detect potential data exfiltration attempts through web channels",
      "parameters": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "description": "User identifier"
          },
          "dataVolume": {
            "type": "number",
            "description": "Volume of data transferred (bytes)"
          },
          "destination": {
            "type": "string",
            "description": "Destination URL or domain"
          },
          "sensitivity": {
            "type": "string",
            "enum": ["low", "medium", "high", "critical"],
            "description": "Sensitivity level to detect"
          }
        },
        "required": ["userId", "destination"]
      }
    },
    {
      "name": "summarize_filtering_activity",
      "description": "Generate comprehensive summary of filtering activity with insights and trends",
      "parameters": {
        "type": "object",
        "properties": {
          "timePeriod": {
            "type": "string",
            "enum": ["day", "week", "month", "quarter"],
            "description": "Period to summarize"
          },
          "includeUsers": {
            "type": "boolean",
            "description": "Include per-user statistics",
            "default": true
          },
          "includeThreats": {
            "type": "boolean",
            "description": "Include threat statistics",
            "default": true
          },
          "includeTrends": {
            "type": "boolean",
            "description": "Include trend analysis",
            "default": true
          }
        },
        "required": ["timePeriod"]
      }
    }
  ],
  "api": {
    "baseUrl": "http://localhost:4036/api",
    "endpoints": {
      "analysis": {
        "analyze": "/analysis/url",
        "batch": "/analysis/batch",
        "history": "/analysis/history/:userId"
      },
      "policies": {
        "list": "/policies",
        "create": "/policies",
        "update": "/policies/:id",
        "delete": "/policies/:id",
        "apply": "/policies/:id/apply"
      },
      "users": {
        "list": "/users",
        "create": "/users",
        "profile": "/users/:id",
        "activity": "/users/:id/activity",
        "override": "/users/:id/override"
      },
      "filtering": {
        "check": "/filter/check",
        "allow": "/filter/allow",
        "block": "/filter/block",
        "categories": "/filter/categories"
      },
      "logs": {
        "access": "/logs/access",
        "blocked": "/logs/blocked",
        "alerts": "/logs/alerts",
        "export": "/logs/export"
      },
      "reports": {
        "summary": "/reports/summary",
        "threats": "/reports/threats",
        "compliance": "/reports/compliance",
        "bandwidth": "/reports/bandwidth",
        "users": "/reports/users"
      },
      "dashboard": {
        "overview": "/dashboard",
        "realtime": "/dashboard/realtime",
        "stats": "/dashboard/stats"
      }
    }
  },
