# BiometricAI API Configuration
PORT=4034
NODE_ENV=development

# Database
MONGODB_URI=mongodb://localhost:27017/biometricai_db

# ML Engine
ML_ENGINE_URL=http://localhost:8034

# ============================================================================
# BIOMETRIC API INTEGRATIONS - Add your API keys here
# ============================================================================

# Face Recognition APIs
# Azure Face API (Microsoft)
AZURE_FACE_API_KEY=your_azure_face_api_key_here
AZURE_FACE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/

# AWS Rekognition
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Google Cloud Vision AI
GOOGLE_CLOUD_VISION_API_KEY=your_google_vision_api_key_here
GOOGLE_CLOUD_PROJECT_ID=your_google_project_id_here

# Fingerprint Recognition
# Suprema BioMini API
SUPREMA_BIOMINI_API_KEY=your_suprema_api_key_here
SUPREMA_BIOMINI_ENDPOINT=https://api.suprema.com/

# Voice Recognition
# Google Speech-to-Text
GOOGLE_SPEECH_API_KEY=your_google_speech_api_key_here

# Behavioral Biometrics
# BioCatch API
BIO_CATCH_API_KEY=your_biocatch_api_key_here
BIO_CATCH_ENDPOINT=https://api.biocatch.com/

# Typing Pattern Analysis
# TypingDNA API
TYPING_DNA_API_KEY=your_typingdna_api_key_here
TYPING_DNA_ENDPOINT=https://api.typingdna.com/

# Additional Biometric APIs
# Face++ API (Alternative face recognition)
FACE_PLUS_PLUS_API_KEY=your_face_plus_plus_api_key_here
FACE_PLUS_PLUS_API_SECRET=your_face_plus_plus_secret_here

# Kairos Face Recognition
KAIROS_APP_ID=your_kairos_app_id_here
KAIROS_APP_KEY=your_kairos_app_key_here

# Voice Biometrics - Respeecher
RESPEECHER_API_KEY=your_respeecher_api_key_here
RESPEECHER_ENDPOINT=https://api.respeecher.com/

# Fingerprint - DigitalPersona
DIGITAL_PERSONA_API_KEY=your_digital_persona_api_key_here
DIGITAL_PERSONA_ENDPOINT=https://api.digitalpersona.com/

# Iris Recognition - IrisGuard
IRIS_GUARD_API_KEY=your_iris_guard_api_key_here
IRIS_GUARD_ENDPOINT=https://api.irisguard.com/

# Behavioral - BehavioSec
BEHAVIO_SEC_API_KEY=your_behavio_sec_api_key_here
BEHAVIO_SEC_ENDPOINT=https://api.behaviosec.com/

# ============================================================================
# EXTERNAL SECURITY INTEGRATIONS
# ============================================================================

# SIEM Integration
SPLUNK_HEC_URL=https://your-splunk-hec-url.com
SPLUNK_HEC_TOKEN=your_splunk_hec_token_here

# Threat Intelligence
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here

# Notification Services
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# Email Service
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=noreply@biometricai.maula.ai

# Push Notifications
FIREBASE_SERVER_KEY=your_firebase_server_key_here
ONESIGNAL_APP_ID=your_onesignal_app_id_here
ONESIGNAL_API_KEY=your_onesignal_api_key_here

# ============================================================================
# ADVANCED SECURITY & MONITORING
# ============================================================================

# Intrusion Detection
SNORT_RULES_PATH=/etc/snort/rules
SURICATA_CONFIG=/etc/suricata/suricata.yaml

# Endpoint Protection
CROWDSTRIKE_API_KEY=your_crowdstrike_api_key_here
CROWDSTRIKE_BASE_URL=https://api.crowdstrike.com/

# Security Orchestration
XSOAR_API_KEY=your_xsoar_api_key_here
XSOAR_BASE_URL=https://your-xsoar-instance.com/

# Log Analysis
ELASTICSEARCH_URL=https://your-elasticsearch-cluster.com
ELASTICSEARCH_API_KEY=your_elasticsearch_api_key_here

# ============================================================================
# COMPLIANCE & AUDIT
# ============================================================================

# SOC 2 Compliance
SOC2_AUDIT_LOG_PATH=/var/log/soc2-audit.log

# GDPR Compliance
GDPR_DATA_RETENTION_DAYS=2555
GDPR_CONSENT_LOG_PATH=/var/log/gdpr-consent.log

# HIPAA Compliance (if applicable)
HIPAA_AUDIT_LOG_PATH=/var/log/hipaa-audit.log
HIPAA_ENCRYPTION_KEY=your_hipaa_encryption_key_here

# ============================================================================
# SECURITY & MONITORING
# ============================================================================

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_here
JWT_EXPIRE=24h

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# Encryption
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Logging
LOG_LEVEL=info
LOG_FILE=/var/log/biometricai.log
