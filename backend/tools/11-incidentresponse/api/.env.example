incidentcommand_AWS_REGION=us-east-1
incidentcommand_AWS_S3_BUCKET=incident-evidence-bucket

# GCP Cloud Storage for Evidence
incidentcommand_GCP_PROJECT_ID=your-gcp-project-id
incidentcommand_GCP_BUCKET_NAME=incident-evidence-bucket
incidentcommand_GCP_CREDENTIALS_PATH=/path/to/credentials.json

# ============================================
# MITRE ATT&CK Integration
# ============================================

# MITRE ATT&CK API
incidentcommand_MITRE_ATTACK_BASE_URL=https://attack.mitre.org/api

# ============================================
# Real-time Configuration
# ============================================

# WebSocket for real-time updates
incidentcommand_WEBSOCKET_ENABLED=true
incidentcommand_WEBSOCKET_PORT=4111

# Redis for caching and real-time
incidentcommand_REDIS_URL=redis://localhost:6379
incidentcommand_REDIS_PASSWORD=

# Rate Limiting
incidentcommand_RATE_LIMIT_WINDOW_MS=60000
incidentcommand_RATE_LIMIT_MAX_REQUESTS=100

# Auto-escalation thresholds
incidentcommand_AUTO_ESCALATE_CRITICAL_MINUTES=15
incidentcommand_AUTO_ESCALATE_HIGH_MINUTES=60
incidentcommand_SLA_CRITICAL_MINUTES=30
incidentcommand_SLA_HIGH_MINUTES=240
