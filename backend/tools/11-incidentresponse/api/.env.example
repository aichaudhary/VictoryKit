PORT=4011
NODE_ENV=development
MONGODB_URI=mongodb://localhost:27017/victorykit_incidentcommand
ML_ENGINE_URL=http://localhost:8011
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3011,https://incidentcommand.maula.ai
