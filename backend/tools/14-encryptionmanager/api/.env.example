# ============================================
# EncryptionManager API Configuration - Tool 14
# Enterprise Key Management & Data Encryption
# ============================================

PORT=4014
NODE_ENV=development
MONGODB_URI=mongodb://localhost:27017/victorykit_encryptionmanager
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3014,https://encryptionmanager.maula.ai
LOG_LEVEL=info

# ============================================
# AWS Key Management Service (KMS)
# https://aws.amazon.com/kms/
# ============================================
ENCRYPTIONMANAGER_AWS_ACCESS_KEY_ID=your-aws-access-key-id
ENCRYPTIONMANAGER_AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
ENCRYPTIONMANAGER_AWS_REGION=us-east-1
ENCRYPTIONMANAGER_AWS_KMS_KEY_ID=your-aws-kms-key-id
ENCRYPTIONMANAGER_AWS_KMS_ENDPOINT=https://kms.us-east-1.amazonaws.com

# ============================================
# Microsoft Azure Key Vault
# https://azure.microsoft.com/en-us/products/key-vault/
# ============================================
ENCRYPTIONMANAGER_AZURE_TENANT_ID=your-azure-tenant-id
ENCRYPTIONMANAGER_AZURE_CLIENT_ID=your-azure-client-id
ENCRYPTIONMANAGER_AZURE_CLIENT_SECRET=your-azure-client-secret
ENCRYPTIONMANAGER_AZURE_KEY_VAULT_NAME=your-key-vault-name
ENCRYPTIONMANAGER_AZURE_KEY_VAULT_URL=https://your-vault.vault.azure.net/

# ============================================
# Google Cloud KMS
# https://cloud.google.com/security-key-management
# ============================================
ENCRYPTIONMANAGER_GCP_PROJECT_ID=your-gcp-project-id
ENCRYPTIONMANAGER_GCP_LOCATION=global
ENCRYPTIONMANAGER_GCP_KEYRING=your-keyring-name
ENCRYPTIONMANAGER_GCP_KEY_ID=your-key-id
ENCRYPTIONMANAGER_GCP_CREDENTIALS_PATH=/path/to/service-account.json

# ============================================
# HashiCorp Vault
# https://www.vaultproject.io/
# ============================================
ENCRYPTIONMANAGER_VAULT_ADDR=https://vault.example.com:8200
ENCRYPTIONMANAGER_VAULT_TOKEN=your-vault-token
ENCRYPTIONMANAGER_VAULT_NAMESPACE=admin
ENCRYPTIONMANAGER_VAULT_SECRET_PATH=secret/data/encryption
ENCRYPTIONMANAGER_VAULT_TRANSIT_PATH=transit

# ============================================
# Thales CipherTrust Manager (Luna HSM)
# https://cpl.thalesgroup.com/encryption/ciphertrust-manager
# ============================================
ENCRYPTIONMANAGER_THALES_HOST=https://ciphertrust.example.com
ENCRYPTIONMANAGER_THALES_USERNAME=your-thales-username
ENCRYPTIONMANAGER_THALES_PASSWORD=your-thales-password
ENCRYPTIONMANAGER_THALES_PARTITION=your-hsm-partition

# ============================================
# Fortanix SDKMS (Software-Defined Key Management)
# https://www.fortanix.com/products/sdkms/
# ============================================
ENCRYPTIONMANAGER_FORTANIX_API_KEY=your-fortanix-api-key
ENCRYPTIONMANAGER_FORTANIX_ENDPOINT=https://sdkms.fortanix.com
ENCRYPTIONMANAGER_FORTANIX_ACCOUNT_ID=your-account-id
ENCRYPTIONMANAGER_FORTANIX_GROUP_ID=your-group-id

# ============================================
# IBM Cloud Key Protect
# https://www.ibm.com/cloud/key-protect
# ============================================
ENCRYPTIONMANAGER_IBM_API_KEY=your-ibm-api-key
ENCRYPTIONMANAGER_IBM_INSTANCE_ID=your-key-protect-instance-id
ENCRYPTIONMANAGER_IBM_REGION=us-south
ENCRYPTIONMANAGER_IBM_ENDPOINT=https://us-south.kms.cloud.ibm.com

# ============================================
# Doppler Secrets Management
# https://www.doppler.com/
# ============================================
ENCRYPTIONMANAGER_DOPPLER_TOKEN=your-doppler-service-token
ENCRYPTIONMANAGER_DOPPLER_PROJECT=your-project-name
ENCRYPTIONMANAGER_DOPPLER_CONFIG=production

# ============================================
# 1Password Secrets Automation
# https://developer.1password.com/
# ============================================
ENCRYPTIONMANAGER_1PASSWORD_TOKEN=your-1password-connect-token
ENCRYPTIONMANAGER_1PASSWORD_SERVER_URL=http://localhost:8080
ENCRYPTIONMANAGER_1PASSWORD_VAULT_ID=your-vault-id

# ============================================
# SSL/TLS Certificate Providers
# ============================================

# Let's Encrypt (https://letsencrypt.org/)
ENCRYPTIONMANAGER_LETSENCRYPT_ACCOUNT_KEY=/path/to/account.key
ENCRYPTIONMANAGER_LETSENCRYPT_ACME_URL=https://acme-v02.api.letsencrypt.org/directory
ENCRYPTIONMANAGER_LETSENCRYPT_EMAIL=admin@example.com

# DigiCert CertCentral (https://www.digicert.com/)
ENCRYPTIONMANAGER_DIGICERT_API_KEY=your-digicert-api-key
ENCRYPTIONMANAGER_DIGICERT_ORG_ID=your-organization-id
ENCRYPTIONMANAGER_DIGICERT_ENDPOINT=https://www.digicert.com/services/v2

# Sectigo (formerly Comodo CA) (https://sectigo.com/)
ENCRYPTIONMANAGER_SECTIGO_LOGIN_NAME=your-sectigo-login
ENCRYPTIONMANAGER_SECTIGO_LOGIN_PASSWORD=your-sectigo-password
ENCRYPTIONMANAGER_SECTIGO_CUSTOMER_URI=your-customer-uri
ENCRYPTIONMANAGER_SECTIGO_ENDPOINT=https://hard.cert-manager.com/ws

# GlobalSign (https://www.globalsign.com/)
ENCRYPTIONMANAGER_GLOBALSIGN_API_KEY=your-globalsign-api-key
ENCRYPTIONMANAGER_GLOBALSIGN_API_SECRET=your-globalsign-api-secret
ENCRYPTIONMANAGER_GLOBALSIGN_ENDPOINT=https://emea.api.hvca.globalsign.com:8443

# ============================================
# Hardware Security Module (HSM) Integrations
# ============================================

# AWS CloudHSM (https://aws.amazon.com/cloudhsm/)
ENCRYPTIONMANAGER_AWS_CLOUDHSM_CLUSTER_ID=your-cloudhsm-cluster-id
ENCRYPTIONMANAGER_AWS_CLOUDHSM_CU_USER=crypto_user
ENCRYPTIONMANAGER_AWS_CLOUDHSM_CU_PASSWORD=your-cu-password

# Google Cloud HSM (https://cloud.google.com/kms/docs/hsm)
ENCRYPTIONMANAGER_GCP_HSM_LOCATION=us-east1
ENCRYPTIONMANAGER_GCP_HSM_KEYRING=hsm-keyring
ENCRYPTIONMANAGER_GCP_HSM_PROTECTION_LEVEL=HSM

# Utimaco HSM (https://utimaco.com/)
ENCRYPTIONMANAGER_UTIMACO_HOST=192.168.1.100
ENCRYPTIONMANAGER_UTIMACO_PORT=3001
ENCRYPTIONMANAGER_UTIMACO_SLOT_ID=0
ENCRYPTIONMANAGER_UTIMACO_PIN=your-hsm-pin

# nCipher/Entrust nShield HSM (https://www.entrust.com/digital-security/hsm)
ENCRYPTIONMANAGER_NCIPHER_MODULE_PATH=/opt/nfast/toolkits/pkcs11/libcknfast.so
ENCRYPTIONMANAGER_NCIPHER_SLOT_ID=0
ENCRYPTIONMANAGER_NCIPHER_PIN=your-nshield-pin

# ============================================
# Encryption Key Escrow & Backup
# ============================================

# AWS S3 for Key Backup (Encrypted)
ENCRYPTIONMANAGER_BACKUP_S3_BUCKET=your-key-backup-bucket
ENCRYPTIONMANAGER_BACKUP_S3_REGION=us-east-1
ENCRYPTIONMANAGER_BACKUP_S3_PREFIX=key-backups/

# Azure Blob Storage for Key Backup
ENCRYPTIONMANAGER_BACKUP_AZURE_ACCOUNT=your-storage-account
ENCRYPTIONMANAGER_BACKUP_AZURE_CONTAINER=key-backups
ENCRYPTIONMANAGER_BACKUP_AZURE_SAS_TOKEN=your-sas-token

# ============================================
# Certificate Transparency & Monitoring
# ============================================

# Certificate Transparency Log Monitors
ENCRYPTIONMANAGER_CT_LOG_MONITOR_ENABLED=true
ENCRYPTIONMANAGER_CT_DOMAINS_TO_MONITOR=example.com,*.example.com

# SSLMate Cert Spotter (https://sslmate.com/certspotter/)
ENCRYPTIONMANAGER_CERTSPOTTER_API_KEY=your-certspotter-api-key
ENCRYPTIONMANAGER_CERTSPOTTER_BASE_URL=https://api.certspotter.com/v1

# crt.sh API (https://crt.sh/)
ENCRYPTIONMANAGER_CRTSH_BASE_URL=https://crt.sh

# ============================================
# Compliance & Audit Integration
# ============================================

# Splunk for Audit Logs (https://www.splunk.com/)
ENCRYPTIONMANAGER_SPLUNK_HEC_TOKEN=your-splunk-hec-token
ENCRYPTIONMANAGER_SPLUNK_HEC_URL=https://splunk.example.com:8088/services/collector
ENCRYPTIONMANAGER_SPLUNK_INDEX=encryption_audit

# Datadog APM (https://www.datadoghq.com/)
ENCRYPTIONMANAGER_DATADOG_API_KEY=your-datadog-api-key
ENCRYPTIONMANAGER_DATADOG_APP_KEY=your-datadog-app-key
ENCRYPTIONMANAGER_DATADOG_SITE=datadoghq.com

# AWS CloudTrail for Audit
ENCRYPTIONMANAGER_CLOUDTRAIL_ENABLED=true
ENCRYPTIONMANAGER_CLOUDTRAIL_TRAIL_NAME=encryption-audit-trail

# ============================================
# Notification & Alerting
# ============================================

# Slack Notifications (https://api.slack.com/)
ENCRYPTIONMANAGER_SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
ENCRYPTIONMANAGER_SLACK_CHANNEL=#encryption-alerts

# PagerDuty (https://www.pagerduty.com/)
ENCRYPTIONMANAGER_PAGERDUTY_ROUTING_KEY=your-pagerduty-routing-key
ENCRYPTIONMANAGER_PAGERDUTY_API_KEY=your-pagerduty-api-key

# Email (SMTP)
ENCRYPTIONMANAGER_SMTP_HOST=smtp.example.com
ENCRYPTIONMANAGER_SMTP_PORT=587
ENCRYPTIONMANAGER_SMTP_USER=encryption-alerts@example.com
ENCRYPTIONMANAGER_SMTP_PASSWORD=your-smtp-password
ENCRYPTIONMANAGER_SMTP_FROM=EncryptionManager <encryption-alerts@example.com>

# ============================================
# AI/ML for Anomaly Detection
# ============================================

# OpenAI (for key usage anomaly detection)
ENCRYPTIONMANAGER_OPENAI_API_KEY=your-openai-api-key
ENCRYPTIONMANAGER_OPENAI_MODEL=gpt-4-turbo-preview

# Azure Anomaly Detector (https://azure.microsoft.com/en-us/products/ai-services/ai-anomaly-detector/)
ENCRYPTIONMANAGER_AZURE_ANOMALY_KEY=your-anomaly-detector-key
ENCRYPTIONMANAGER_AZURE_ANOMALY_ENDPOINT=https://your-resource.cognitiveservices.azure.com/

# ============================================
# PKI & Certificate Authority Integration
# ============================================

# Internal PKI / Microsoft AD CS
ENCRYPTIONMANAGER_ADCS_SERVER=ca.example.com
ENCRYPTIONMANAGER_ADCS_TEMPLATE=WebServer
ENCRYPTIONMANAGER_ADCS_USERNAME=admin
ENCRYPTIONMANAGER_ADCS_PASSWORD=your-adcs-password

# EJBCA (https://www.ejbca.org/)
ENCRYPTIONMANAGER_EJBCA_URL=https://ejbca.example.com/ejbca/ejbca-rest-api
ENCRYPTIONMANAGER_EJBCA_CLIENT_CERT=/path/to/client.crt
ENCRYPTIONMANAGER_EJBCA_CLIENT_KEY=/path/to/client.key

# Venafi Trust Protection Platform (https://www.venafi.com/)
ENCRYPTIONMANAGER_VENAFI_URL=https://venafi.example.com/vedsdk
ENCRYPTIONMANAGER_VENAFI_USERNAME=your-venafi-username
ENCRYPTIONMANAGER_VENAFI_PASSWORD=your-venafi-password
ENCRYPTIONMANAGER_VENAFI_POLICY_FOLDER=\\VED\\Policy\\Certificates

# ============================================
# Key Rotation Scheduling
# ============================================
ENCRYPTIONMANAGER_DEFAULT_KEY_ROTATION_DAYS=90
ENCRYPTIONMANAGER_CERT_EXPIRY_WARNING_DAYS=30
ENCRYPTIONMANAGER_KEY_EXPIRY_WARNING_DAYS=14
ENCRYPTIONMANAGER_AUTO_ROTATE_ENABLED=true
