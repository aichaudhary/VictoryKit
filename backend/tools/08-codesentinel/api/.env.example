# ═══════════════════════════════════════════════════════════════════════════════
# APPLICATION CONFIGURATION
# ═══════════════════════════════════════════════════════════════════════════════

# Node Environment
NODE_ENV=development
# Options: development, staging, production, test

# Application Identity
APP_NAME=codesentinel
APP_DISPLAY_NAME=CodeSentinel
APP_VERSION=8.0.0
APP_DESCRIPTION=AI-Powered Static Application Security Testing Engine

# Server Configuration
PORT=4008
HOST=0.0.0.0
API_PREFIX=/api/v1

# Frontend Configuration
FRONTEND_PORT=3008
FRONTEND_URL=http://localhost:3008
# Production: https://codesentinel.maula.ai

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3008,http://localhost:3000,https://codesentinel.maula.ai,https://maula.ai
CORS_CREDENTIALS=true

# Trust Proxy (for reverse proxy setups)
TRUST_PROXY=true

# ═══════════════════════════════════════════════════════════════════════════════
# DATABASE CONFIGURATION
# ═══════════════════════════════════════════════════════════════════════════════

# MongoDB Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/victorykit?retryWrites=true&w=majority
MONGODB_DB_NAME=victorykit

# MongoDB Options
MONGODB_MAX_POOL_SIZE=50
MONGODB_MIN_POOL_SIZE=10
MONGODB_CONNECT_TIMEOUT=10000
MONGODB_SOCKET_TIMEOUT=45000
MONGODB_SERVER_SELECTION_TIMEOUT=30000
MONGODB_HEARTBEAT_FREQUENCY=10000
MONGODB_RETRY_WRITES=true
MONGODB_RETRY_READS=true

# Redis Configuration (for caching and sessions)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=8
REDIS_KEY_PREFIX=codesentinel:
REDIS_TLS_ENABLED=false

# Redis Cluster (Production)
REDIS_CLUSTER_ENABLED=false
REDIS_CLUSTER_NODES=redis-node1:6379,redis-node2:6379,redis-node3:6379

# ═══════════════════════════════════════════════════════════════════════════════
# AUTHENTICATION & SECURITY
# ═══════════════════════════════════════════════════════════════════════════════

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-min-256-bits-for-production
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d
JWT_ISSUER=codesentinel.maula.ai
JWT_AUDIENCE=victorykit-users
JWT_ALGORITHM=HS256

# Session Configuration
SESSION_SECRET=your-session-secret-key-for-codesentinel
SESSION_NAME=codesentinel_session
SESSION_MAX_AGE=86400000
SESSION_SECURE=true
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=strict

# API Keys (Internal)
INTERNAL_API_KEY=your-internal-api-key-for-service-communication
WEBHOOK_SECRET=your-webhook-signing-secret
ENCRYPTION_KEY=your-32-character-encryption-key!

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_SKIP_SUCCESSFUL=false
SCAN_RATE_LIMIT_PER_HOUR=50
API_RATE_LIMIT_PER_MINUTE=100

# Security Headers
HELMET_ENABLED=true
CONTENT_SECURITY_POLICY=default-src 'self'; script-src 'self' 'unsafe-inline'

